<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.SqlConnectionFactory, EntityFramework" />
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
  <connectionStrings>
    <add name="dentnedEntities" connectionString="metadata=res://*/Entity.Model_dentned.csdl|res://*/Entity.Model_dentned.ssdl|res://*/Entity.Model_dentned.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=192.168.1.57;initial catalog=dentned;user id=sa;password=123456;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
  </connectionStrings>
  <appSettings>
    <!-- language -->
    <add key="language" value="it" />
    <!-- patient content folder -->
    <add key="patientsDatadir" value="..\..\..\..\DentneDTemp\Datadir" />
    <!-- patient attachements folder -->
    <add key="patientsAttachmentsdir" value="..\..\..\..\DentneDTemp\Attachmentsdir" />
    <!-- invoices folder -->
    <add key="invoicesdir" value="..\..\..\..\DentneDTemp\Invoices" />
    <!-- temporary folder -->
    <add key="tmpdir" value="..\..\..\..\DentneDTemp\Temp" />
    <!-- do secure delete (needs the srm.exe executable) -->
    <add key="doSecureDelete" value="false" />
    <!-- backup script -->
    <add key="backupscript" value="backup\db-backup.bat" />
    <!-- password for protected forms -->
    <add key="formspassword" value="123456" />
    <!-- set the patient payment reference: 'T' if you plain to refer payment to treatments, 'I' for invoices -->
    <add key="paymentReference" value="T" />
    <!-- reset the patient treatments filter on patient change -->
    <add key="resetPatientstreatmentsFilterOnChange" value="false"/>
    <!-- calendar highlight start end end hours -->
    <add key="calendarDayHourBegin" value="8" />
    <add key="calendarDayHourEnd" value="19" />
    <!-- calendar first day of the week -->
    <add key="calendarFirstDayOfTheWeek" value="Monday" />
    <!-- calendar include Saturaday  -->
    <add key="calendarWeekIncludeSaturday" value="true" />
    <!-- calendar include Sunday  -->
    <add key="calendarWeekIncludeSunday" value="false" />
    <!-- print model implementation dll (implements IDentneDPrintModel) -->
    <add key="printModel" value="DentneDPrintModelDefault.dll" />
    <!-- set the startup size, format: 'WxH' (W is width, H is height), or 'maximized' to maximize the window -->
    <add key="sizeOnStartup" value="1024x768" />
    <!-- show invoice and estimates total on bottom -->
    <add key="showInvoicesEstimatesTotal" value="true" />
  </appSettings>
</configuration>